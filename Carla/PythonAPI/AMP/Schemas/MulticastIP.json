{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Multicast IP",
    "description": "A multicast IP in the local organization use range of 239.255/16",
    "type": "string",
    "examples": [
        "239.255.255.255",
        "239.255.128.128",
        "239.255.64.64",
        "239.255.0.1"
    ],
    "maxLength": 15,
    "minLength": 11,
    "pattern": "^239.255(?:\\.(?:25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]\\d?|0)){2}$"
}