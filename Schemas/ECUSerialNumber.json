{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "ECU Serial Number",
    "description": "The serial number of the ECU.",
    "type": "string",
    "examples": [
        "1A2B3C4D",
        "D4C3B2A1"
    ],
    "maxLength": 30,
    "minLength": 1,
    "pattern": "^[a-zA-Z0-9_]+$"
}